    <form action="/register" method="post">
        <table class="form-register">
            <caption align="top" style="color: white; text-align:center;"><h1 id="TitleMister">REGISTRO</h1></caption>
            <tr>
              <td class="texto"><h4><label for="email">INGRESE SU CORREO</label></h4></td>
            </tr>
            <tr>
                <td class="texto"><input type="email" name="email" id="email" maxlength="30" minlength="8" required></td>
            </tr>

            <tr>
                <td class="texto"><h4><label for="password">INGRESE SU CONTRASEÑA</label></h4></td>
            </tr>
            <tr>
                  <td class="texto"><input type="password" name="password1" id="password1" maxlength="20" minlength="6" required></td>
            </tr>

            <tr>
                <td class="texto"><h4><label for="password2">CONFIRME SU CONTRASEÑA</label></h4></td>
            </tr>
            <tr>
                <td class="texto"><input type="password" name="password2" id="password2" maxlength="20" minlength="6" required></td>
            </tr>

            <tr>
                <td class="texto"><h4><label for="preg_seg">INGRESE SU PREGUNTA DE SEGURIDAD</label></h4></td>
            </tr>
            <tr>
                <td class="texto"><input type="text" name="preg_seg" id="preg_seg" maxlength="30" minlength="6" required></td>
            </tr>

            <tr>
                <td class="texto"><h4><label for="resp_seg">INGRESE SU RESPUESTA DE SEGURIDAD</label></h4></td>
            </tr>
            <tr>
                <td class="texto"><input type="text" name="resp_seg" id="resp_seg" maxlength="30" minlength="6" required></td>
            </tr>
        </table><br>
        <div class="g-recaptcha" data-sitekey="6LeAHVApAAAAAM1NdEZsyfINrFhnHr6Ggdr8eM3Y"></div><br>
        <input class="botons" type="submit" value="Registrate"><br><br>
        <a style="margin-left: 49%;" href="/loginclientes"><img src="/img/back.png" width="30" id="boton"></a><hr style="color: white"><br>
            
    </form>

